/**about.wxss**/
@import "../../wxParse/wxParse.wxss";
.about{
  padding:30rpx;
}
.about-top{
  text-align: center;
}
.about-img{
  width: 300rpx;
  height: 300rpx;
  display: block;
  margin: 20rpx auto 0;
}
.about-title{
  font-size:40rpx;
  margin:20rpx 0;
}
.about-text{
  text-indent:2em;
  margin-bottom:20rpx;
  line-height: 60rpx;
}
.about-text,.about-tab,.add,.business{
  font-size: 16px;
}
.numtab{
  margin-bottom:7rpx;
}
.about-tab{
  margin:7rpx 0;
}